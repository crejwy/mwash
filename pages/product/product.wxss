.notice{
  height: 50rpx;
   /* overflow: hidden;  */

}
.notice-title{
  display: block;
  font-size: 14px;
  width: 100rpx; 
  height: 100%; 
  float: left;
}
.notice-content{
  display: block;
  /* width: 400rpx;  */
  height: 100%; 
  float: left;
}
.notice-calandar {
  display: block;
  font-size: 14px;
  width: 200rpx;
  float: left;
  margin-left: 20rpx;
}
.notice-calandar picker{
  display: inline-block;
  width: 50rpx;
  margin-left: 10rpx;
}

/*图片  */
.backimg{
  width: 100%;
  height: 500rpx;
}

.placeofissue{
  position: relative;
}
.select_placeofissue{
  position: absolute;
  top: 250rpx;
  display: block;
  width: 400rpx;
  margin-left: 100rpx;
  color: white;
  /* margin: 200rpx auto; */
}
.select_placeofissue .picker{
  display: inline-block;
  border: solid 1px gray;
  width: 160rpx;
  text-align: right;
  border-radius: 10rpx;
  cursor: pointer;
}


/*product  */

.product{
  height: 100rpx;
  border-bottom: solid 1px gray;
  position: relative;
}
.product_price{
  display: block;
  width: 30%;
  height: 100rpx;
  line-height: 100rpx;
  /* background: red; */
  float: left;
}
.product_info{
  display: block;
  width: 70%;
  float: left;
}
